# AI API Keys Configuration
# Copy this file to .env and add your actual API keys
# NEVER commit .env to git!

# Groq API (Fast inference with Llama models)
GROQ_API_KEY=your_groq_api_key_here

# LangChain API (Tracing and monitoring)
LANGCHAIN_API_KEY=your_langchain_api_key_here
LANGCHAIN_TRACING_V2=true
LANGCHAIN_PROJECT=automl-insight

# OpenAI API (GPT-4 for advanced analysis)
OPENAI_API_KEY=your_openai_api_key_here

# Google Gemini API (Alternative LLM)
GEMINI_API_KEY=your_gemini_api_key_here

# AI Configuration
DEFAULT_LLM_PROVIDER=groq  # Options: groq, openai, gemini
DEFAULT_MODEL=llama-3.1-70b-versatile  # Groq model
ENABLE_AI_INSIGHTS=true
AI_TEMPERATURE=0.3  # Lower = more deterministic
AI_MAX_TOKENS=2000
